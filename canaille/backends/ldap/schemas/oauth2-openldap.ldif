              oauthIdTokenEncryptedResponseAlg $
              oauthIdTokenEncryptedResponseEnc $
              oauthUserinfoSignedResponseAlg $
              oauthUserinfoEncryptedResponseAlg $
              oauthUserinfoEncryptedResponseEnc $
              oauthDefaultMaxAge $
              oauthRequireAuthTime $
